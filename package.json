{
  "name": "boot4-tutorial",
  "version": "1.0.0",
  "main": "index.js",
  "author": "GT",
  "license": "MIT",
  "scripts":{
    "setup-css": "cpx \"node_modules/bootstrap/scss/**\" -u \"src/vendor/scss/\"",
    "setup-js": "cpx \"node_modules/bootstrap/js/**\" -u \"src/vendor/js/\"",
    "setup": "yarpm run setup-css && yarpm run setup-js",
    "reset": "npx rimraf src/vendor",
    "serve": "parcel src/*.html --port 1234",
    "watch": "parcel watch src/index.html",
    "build": "parcel build src/index.html --out-dir dist"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.2",
    "scss": "^0.2.4",
    "yarpm": "^0.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "cpx": "^1.5.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.22.7"
  },
  "browser": {
    "browsers": [
      "> 1%",
      "not dead"
    ]
  }
}
